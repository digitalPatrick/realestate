    <script src="app/directives/directives.module.js"></script>
    <script src="app/directives/directive.js"></script>

    <!-- home -->
    <script src="app/home/home.module.js"></script>
    <script src="app/home/home.js"></script>

    <!-- layout -->
    <script src="app/layout/layout.module.js"></script>
    <script src="app/layout/layout.js"></script>
    <script src="app/layout/header.js"></script>
    <!-- contact -->
    <script src="app/contact/contact.module.js"></script>
    <script src="app/contact/contact.js"></script>

    <!-- Agent -->
    <script src="app/AgentLandingPage/agent.module.js"></script>
    <script src="app/AgentLandingPage/agent.js"></script>

