<html ng-app="app">
<!--<![endif]-->

<head>
    <style>
        /* This helps the ng-show/ng-hide animations start at the right place. */
        /* Since Angular has this but needs to load, this gives us the class early. */
        .ng-hide {
            display: none !important;
        }
    </style>

    <base href="/">
    <meta charset="utf-8" />
    <title data-ng-bind="title">DigitalPatrick</title>
    <meta name="keywords" content="HTML5,CSS3,Template" />
    <meta name="description" content="" />
    <meta name="Author" content="Leon PAtrick [www.digitalpatrick.com]" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />

    <!-- mobile settings -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=0" />

    <!-- Favicon -->
    <link rel="icon" href="@Url.Content("favicon.ico")" />

    <!-- WEB FONTS -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700,800&amp;subset=latin,latin-ext,cyrillic,cyrillic-ext" rel="stylesheet" type="text/css" />

    <!-- CORE CSS -->
    <link href="dist/css/core.css" rel="stylesheet" />
    <link href="lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />


    <!-- THEME CSS -->

    <link href="dist/css/essential.css" rel="stylesheet" />


    <script src="http://ajax.aspnetcdn.com/ajax/mobileservices/MobileServices.Web-1.2.5.min.js"></script>

</head>
<body class="smoothscroll">
    @await Html.PartialAsync("_TopBar")
    

    <div ng-view autoscroll="true">

    </div>

    @await Html.PartialAsync("_FooterPartial")

    <a href="#" id="toTop"></a>

    <!-- inject-vendor:js -->
